<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="400.0" minWidth="400.0" stylesheets="@application.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hu.unideb.inf.lali123.controller.MenuController">
   <children>
      <GridPane layoutX="1.0" layoutY="1.0" prefHeight="400.0" prefWidth="400.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="startButton" minHeight="50.0" minWidth="135.0" mnemonicParsing="false" onAction="#openGameBoard" text="Start Game" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Button minHeight="50.0" minWidth="135.0" mnemonicParsing="false" text="High Score" onAction="#openHighScore" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Button fx:id="optionsButton" minHeight="50.0" minWidth="135.0" mnemonicParsing="false" onAction="#openOptions" text="Options" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Button fx:id="exitButton" minHeight="50.0" minWidth="135.0" mnemonicParsing="false" onAction="#exitGame" text="Exit" GridPane.columnIndex="1" GridPane.rowIndex="4" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
